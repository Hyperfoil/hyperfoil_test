name: example
agents:
{% for agent in groups[hyperfoil_agent_group] %}
  {{ agent }}: {{ hostvars[agent]['ansible_hostname'] }}:{{ hyperfoil_agent_port }}
{% endfor %}
http:
  baseUrl: {{ test_domain | default("http://hyperfoil.io", true) }}
phases:
- example:
    atOnce:
      users: 1
      scenario:
      - test:
        - httpRequest:
            method: GET
            path: /
